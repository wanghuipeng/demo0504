.banner,
.banner image {
    width: 750rpx;
    height: 400rpx;
}

.newsflash {
    background-color: #fff;
    padding: 20rpx 30rpx;
    display: flex;
    flex-direction: row;
    align-items: center
}

.newsflash .left image {
    width: 104rpx;
    height: 73rpx;
}

.newsflash .right {
    width: 560rpx;
    height: 110rpx;
    overflow: hidden;
}

.newsflash .right .item {
    padding-left: 60rpx;
    font-size: 28rpx
}

.newsflash .right .item view {
    position: relative
}

.newsflash .right .item .dot {
    position: absolute;
    width: 10rpx;
    height: 10rpx;
    display: block;
    left: -20rpx;
    top: 15rpx;
    background-color: #ff5900;
    border-radius: 20rpx;
}

.applyForm {
    text-align: center;
    background-color: #fff;
    margin-top: 20rpx;
    padding: 20rpx 30rpx
}

.applyForm .title {
    font-size: 30rpx;
}

.applyForm .medal {
    background-color: #ff7f5b;
    color: #fff;
    border-radius: 4px;
    padding: 0 10rpx;
    margin-right: 15rpx
}

.applyForm .slider {
    margin-top: 20rpx;
}

.applyForm .slider .title .orange {
    font-size: 60rpx;
    color: #ff7f5b;
    margin-right: 10rpx;
}

.applyForm .slider .title {
    text-align: center;
}

.applyForm .body-view {
    position: relative;
}

.applyForm .body-view .left {
    position: absolute;
    left: 40rpx;
    top: -25rpx;
    color: #999;
    font-size: 26rpx;
}

.applyForm .body-view .right {
    position: absolute;
    right: 40rpx;
    top: -25rpx;
    color: #999;
    font-size: 26rpx;
}

.apply {
    background-color: #ffb323;
    color: #fff;
    border-radius: 2px;
    background-color: #ffb323;
    line-height: 80rpx;
    font-size: 36rpx;
    margin: 50rpx 40rpx;
}

.mess {
    display: flex;
    flex-direction: row;
    justify-content: space-around;
}

.mess .item {
    position: relative;
    font-size: 26rpx;
    color: #666;
}

.mess .item::before {
    content: "";
    width: 1px;
    height: 70rpx;
    background-color: #e8e8e8;
    position: absolute;
    top: 10rpx;
    left: -80rpx;
}

.mess .item:first-child::before {
    display: none;
}

.mess .item text {
    display: block;
    margin: 5rpx 0;
}

.titleBar {
    text-align: center;
    margin: 30rpx auto;
    color: #333;
    font-weight: 600;
    font-size: 34rpx;
    position: relative;
}

.titleBar::before {
    content: "";
    height: 1px;
    width: 100rpx;
    background-color: #d8d8d8;
    position: absolute;
    top: 25rpx;
    left: 140rpx;
}

.titleBar::after {
    content: "";
    height: 1px;
    width: 100rpx;
    background-color: #d8d8d8;
    position: absolute;
    top: 25rpx;
    right: 140rpx;
}

.titleBar .hot,
.titleBar .advantage {
    position: relative;
}

.titleBar .hot::before {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    display: block;
    left: 250rpx;
    top: 5rpx;
    content: "";
    background-image: url('data:image/png;base64,');
    background-size: 40rpx;
}

.titleBar .advantage::before {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    display: block;
    left: 250rpx;
    top: 5rpx;
    content: "";
    background-image: url('data:image/png;base64,');
    background-size: 40rpx;
}

.titleBar .hot .title {
    padding-left: 50rpx;
}

.hotPoduct {
    padding: 30rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    background-color: #fff;
    color: #fff;
    position: relative;
}

.hotPoduct .item {
    background-repeat: no-repeat;
    background-size: 100% 100%;
    font-size: 30rpx;
    text-align: center;
    border-radius: 2px;
    width: 220rpx;
    padding: 10rpx 0;
    position: relative;
}

.hotPoduct .text1 {
    position: absolute;
    top: 8rpx;
    left: 4rpx;
    font-size: 22rpx;
    -webkit-transform: rotate(-40deg);
    transform: rotate(-40deg);
}

.hotPoduct .text2,
.hotPoduct .text3,
.hotPoduct .text4 {
    display: block;
}

.ours {
    background-color: #fff;
    display: flex;
    flex-direction: row;
    justify-content: center;
    flex-wrap: wrap;
}

.ours .item {
    width: 50%;
    font-size: 30rpx;
    display: flex;
    flex-direction: column;
    position: relative;
    padding: 30rpx 30px 30rpx 100rpx;
    box-sizing: border-box;
    color: #999;
    border-left: 1rpx solid #f2f2f2;
    border-bottom: 1rpx solid #f2f2f2;
}

.ours .item:first-child {
    border-left: none;
}

.ours .item:nth-child(3) {
    border-left: none;
    border-bottom: none
}

.ours .item:last-child {
    border-bottom: none
}

.ours .item .title {
    font-size: 32rpx;
    color: #333;
    margin-bottom: 5rpx;
}

.ours .item image {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    left: 30rpx;
    top: 30rpx;
}

.footer {
    text-align: center;
}

.footer image {
    width: 200rpx;
    height: 54rpx;
    margin: 30rpx auto;
}

.nav {
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    display: flex;
    flex-direction: row;
    background-color: #f0f0f0;
    border-radius: 20px;
}

.nav>view {
    width: 50%;
    text-align: center;
    border-radius: 20px;
}

.active {
    background-color: #007eff;
    color: #fff;
}

.default {
    background-color: #f0f0f0;
    color: #999;
}

.show {
    display: block;
    text-align: center;
    line-height: 200rpx;
}

.hidden {
    display: none;
    text-align: center;
    line-height: 200px;
}

.form .section {
    margin-top: 40rpx;
    position: relative;
}

.form .section .getCode {
    position: absolute;
    right: -10rpx;
    font-size: 30rpx;
    top: -105rpx;
    height: 50rpx;
    z-index: 100;
    color: #007eff;
    background-color: transparent
}

.form .section input {
    background-color: #f0f0f0;
    font-size: 30rpx;
    border-radius: 20px;
    padding: 0rpx 30rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: left;
    box-sizing: border-box;
}

.form .section.location image {
    width: 30rpx;
    height: 33rpx;
    position: absolute;
    right: 30rpx;
    top: 20rpx;
    z-index: 100;
}

.form .section.check {
    font-size: 30rpx;
    display: flex;
    flex-direction: row;
}

.form .section.check image {
    width: 33rpx;
    height: 33rpx;
    position: absolute;
    left: 53rpx;
    top: 17rpx;
    z-index: 100;
}

.form .section.check .text {
    height: 70rpx;
    line-height: 70rpx;
    justify-content: center;
    width: 100%;
    padding-left: 100rpx;
}

.city {
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 10000;
    background-color: #fff;
}

.modal {
    background-color: #fff;
    width: 80%;
    position: fixed;
    z-index: 300;
    left: 50%;
    top: 50%;
    margin-left: -40%;
    margin-top: -57%;
    padding: 30rpx;
    padding-bottom: 100rpx;
    box-sizing: border-box;
    border-radius: 10px;
}

.modal .btn-groups {
    width: 100%;
    position: absolute;
    left: 0;
    bottom: 0;
}

.modal .btn-groups button {
    border: none;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 34rpx;
    width: 50%;
    display: inline-block;
}


/* 隐藏button自身的边框 */

button[class="getCode"]::after {
    border: 0;
}

.btn-groups {
    border-top: 1px solid #eee
}

.btn-groups button::after {
    border: 0;
}

.btn-groups button {
    background-color: transparent
}

.btn-groups button:last-child {
    border-left: 1px solid #eee
}